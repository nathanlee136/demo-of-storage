<div id="<%= dom_id book %>">
  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= book.description %>
  </p>

  <p>
    <strong>Link:</strong>
    <%= book.link %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= book.date %>
  </p>

</div>
